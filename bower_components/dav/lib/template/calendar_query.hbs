<c:calendar-query xmlns:c="urn:ietf:params:xml:ns:caldav"
                  xmlns:cs="http://calendarserver.org/ns/"
                  xmlns:d="DAV:">
  <d:prop>
    {{#each props}}
      {{prop this}}
    {{/each}}
  </d:prop>
  <c:filter>
    {{#each filters}}
      {{filter this}}
    {{/each}}
  </c:filter>
  {{#if timezone}}
    <c:timezone>{{timezone}}</c:timezone>
  {{/if}}
</c:calendar-query>
