<d:sync-collection xmlns:c="urn:ietf:params:xml:ns:caldav"
                   xmlns:card="urn:ietf:params:xml:ns:carddav"
                   xmlns:d="DAV:">
  <d:sync-level>{{syncLevel}}</d:sync-level>
  <d:sync-token>{{syncToken}}</d:sync-token>
  <d:prop>
    {{#each props}}
      {{prop this}}
    {{/each}}
  </d:prop>
</d:sync-collection>
